{
  "name": "JoinedTogether",
  "version": "1.0.0",
  "description": "GMTK Game Jam 2021",
  "main": "build/main.js",
  "scripts": {
    "download-phaser-typedef": "download --out typings https://raw.githubusercontent.com/photonstorm/phaser/master/types/phaser.d.ts",
    "superclean": "rimraf .cache node_modules dist",
    "clean": "rimraf build dist",
    "serve": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js"
  },
  "homepage": "https://github.com/Golen87/JoinedTogether",
  "build": {
    "appId": "joinedtogether",
    "productName": "Joined Together",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": "deb"
    },
    "deb": {},
    "files": [
      "build/*"
    ]
  },
  "keywords": [
    "phaser"
  ],
  "author": {
    "name": "MÃ¥ns Gezelius",
    "email": "mansgez@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.54.0",
    "phaser3-rex-plugins": "^1.1.51",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "download-cli": "^1.1.1",
    "html-webpack-plugin": "^5.3.1",
    "javascript-obfuscator": "^2.13.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-obfuscator": "^3.3.1",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "staticFiles": {
    "staticPath": {
      "staticPath": "src/assets",
      "staticOutDir": "assets"
    },
    "watcherGlob": "**"
  }
}
